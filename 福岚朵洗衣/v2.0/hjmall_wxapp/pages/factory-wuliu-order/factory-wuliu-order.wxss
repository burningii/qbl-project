.order-no-container {
    display: flex;
    flex-direction: column;
    align-content: center;
    align-items: center;
}

.order-no-list {
    width: 95%;
    display: flex;
    justify-content: space-between;
    align-items: center;
    /*border: 1px solid #e9e9e9;*/
    padding: 35rpx;
    border-radius: 8px;
    /*margin-top: 25rpx;*/
