.btn-add-shape-wrapper {
    text-align: center;
    margin-top: 100rpx;
}

.order-no-container {
    display: flex;
    flex-direction: column;
    align-content: center;
    align-items: center;
}
.order-no-list {
    width: 95%;
    display: flex;
    justify-content: space-between;
    align-items: center;
    border: 1px solid #e9e9e9;
    padding: 35rpx;
    border-radius: 8px;
    margin-top: 25rpx;
    background-color: #fff;
}

.order-no-list>view{
    width: 33%;
    font-weight: 700;
}

.order-no-list>view>text{
    font-weight: normal;
}

.no-orderlist-wrapper {
    text-align: center;
    margin: 180rpx  0 0 0;
    color: #888888;
}

.order-no-list>view:nth-child(2){
    width: 12%;
}